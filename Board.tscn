script/source = "extends Node2D


onready var Scoreboard = $BoardScoreboard
onready var Pellets = $Walls/Pellets


func _ready():
	# Connect the pellets' collected signal to the scoreboard
	for pellet in Pellets.get_children():
		pellet.connect( \"pellet_collected\", Scoreboard, \"add_score\" )
"

